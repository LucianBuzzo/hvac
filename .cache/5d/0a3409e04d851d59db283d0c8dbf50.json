nterval(interval)\n  }, [])\n\n  const send = () => {\n    fetch('./pin/' + pin)\n  }\n\n  return (\n    <div className='App'>\n      <ResetStyles />\n      <ThemeProvider theme={themes.default}>\n        <div\n\n          style={{\n\n            padding: '5rem',\n\n            background: 'teal',\n\n            height: '100%',\n            position: 'fixed',\n            width: '100%'\n          }}\n\n        >\n          <NumberField value={pin} onChange={setPin} />\n          <br />\n          <br />\n          <Button onClick={send}>Test</Button>\n          <br />\n          <br />\n          <TextArea\n            width={600}\n            height={600}\n            value={JSON.stringify(pinData, null, 4)} onChange={console.log} />\n\n        </div>\n      </ThemeProvider>\n    </div>\n  )\n}\n\nconst rootElement = document.getElementById('root')\n\nReactDOM.render(<App />, rootElement)\n"},"lineCount":null}},"error":null,"hash":"a380cd25543fe4b0e23a92d13e3e469a","cacheData":{"env":{}}}